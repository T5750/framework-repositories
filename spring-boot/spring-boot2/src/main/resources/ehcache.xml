<config xmlns='http://www.ehcache.org/v3' xmlns:jsr107='http://www.ehcache.org/v3/jsr107'>
	<service>
		<jsr107:defaults enable-statistics="true"/>
	</service>
	<cache alias="employeeCache">
		<key-type>java.lang.Integer</key-type>
		<value-type>t5750.springboot2.model.Employee</value-type>
		<expiry>
			<ttl unit="seconds">10000</ttl>
		</expiry>
		<listeners>
			<listener>
				<class>t5750.springboot2.listener.CustomCacheEventListener</class>
				<event-firing-mode>ASYNCHRONOUS</event-firing-mode>
				<event-ordering-mode>UNORDERED</event-ordering-mode>
				<events-to-fire-on>CREATED</events-to-fire-on>
				<events-to-fire-on>UPDATED</events-to-fire-on>
				<events-to-fire-on>EXPIRED</events-to-fire-on>
				<events-to-fire-on>REMOVED</events-to-fire-on>
				<events-to-fire-on>EVICTED</events-to-fire-on>
			</listener>
		</listeners>
		<resources>
			<heap unit="entries">2000</heap>
			<offheap unit="MB">100</offheap>
		</resources>
	</cache>
	<cache alias="studentCache">
		<key-type>java.lang.String</key-type>
		<value-type>t5750.springboot2.model.Student</value-type>
		<expiry>
			<ttl unit="seconds">10000</ttl>
		</expiry>
		<listeners>
			<listener>
				<class>t5750.springboot2.listener.CustomCacheEventListener</class>
				<event-firing-mode>ASYNCHRONOUS</event-firing-mode>
				<event-ordering-mode>UNORDERED</event-ordering-mode>
				<events-to-fire-on>CREATED</events-to-fire-on>
				<events-to-fire-on>UPDATED</events-to-fire-on>
				<events-to-fire-on>EXPIRED</events-to-fire-on>
				<events-to-fire-on>REMOVED</events-to-fire-on>
				<events-to-fire-on>EVICTED</events-to-fire-on>
			</listener>
		</listeners>
		<resources>
			<heap unit="entries">2000</heap>
			<offheap unit="MB">100</offheap>
		</resources>
	</cache>
</config>